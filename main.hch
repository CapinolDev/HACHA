program main start
	
	window init 500 500 "Game"
	
	gameloop
		initdraw
		bgclear WHITE
		window draw
		als key w {
			drawtext "WDOWN" 100 100 40 BLACK
		as}
		als key r {
			drawtext "RDOWN" 100 100 40 BLACK
		as}
		als mouse right {
			die 1
		as}
		enddraw
		
	loopend
	window close
program main end
